{% if section.settings.video-chanel == 'bgvideo' %}
    
    <div data-poster-url="{{'video_Demo_toluca_small-poster-00001.jpg' |  asset_url }}" data-video-urls="{{ section.settings.video_link-2.sources.first.url }}" data-autoplay="true" data-loop="true" data-wf-ignore="true" class="background-video w-background-video w-background-video-atom">
      <video id="f297ed57-1a6f-62ac-3514-eb810a43d7ab-video" autoplay="" loop="" style="background-image:url({{'video_Demo_toluca_small-poster-00001.jpg' |  asset_url }})" muted="" playsinline="" data-wf-ignore="true" data-object-fit="cover">
        <source src="{{ section.settings.video_link-3.sources.first.url }}" data-wf-ignore="true">
      </video>
      <noscript>
        <style>
          [data-wf-bgvideo-fallback-img] {
            display: none;
          }
          @media (prefers-reduced-motion: reduce) {
            [data-wf-bgvideo-fallback-img] {
              position: absolute;
              z-index: -100;
              display: inline-block;
              height: 100%;
              width: 100%;
              object-fit: cover;
            }
          }
        </style>
        <img data-wf-bgvideo-fallback-img="true" src="videos/video_Demo_toluca_small-poster-00001.jpg">
      </noscript>
      <div aria-live="polite">
        <button type="button" data-w-bg-video-control="true" aria-controls="f297ed57-1a6f-62ac-3514-eb810a43d7ab-video" class="w-backgroundvideo-backgroundvideoplaypausebutton w-background-video--control">
          <span>
            <img loading="lazy" src="https://uploads-ssl.webflow.com/6022af993a6b2191db3ed10c/628299f8aa233b83918e24fd_Pause.svg" alt="Pause video">
          </span>
          <span hidden="">
            <img loading="lazy" src="https://uploads-ssl.webflow.com/6022af993a6b2191db3ed10c/628298b20ae0236682d4b87f_Play-24.svg" alt="Play video">
          </span>
        </button>
      </div>
    </div>
{% endif %}

{% if section.settings.video-chanel == 'youtubevideo' or section.settings.video-chanel == 'shopifyvideo' %}

    <div class="seccvideo">
      <div class="custom-container">

        <div class="maintitle-cont">
          <h1 class="maintitle white">{{ section.settings.title }}</h1>
        </div>

        {% if section.settings.video-chanel == 'youtubevideo' %}
          <div style="padding-top:56.17021276595745%" class="w-embed-youtubevideo iframyoutube">
            <iframe src="{{ section.settings.Video_link }}" frameborder="0" style="position:absolute;left:0;top:0;width:100%;height:100%;pointer-events:auto" allow="autoplay; encrypted-media" allowfullscreen="" title="Kit de juego Toluca FC 2024/25 | New Balance"></iframe>
          </div>
        {% endif %}

        {% if section.settings.video-chanel == 'shopifyvideo' %}
          <div class="videolibreriashopify w-embed w-iframe">
            <iframe style="position: absolute; top: 0px; left: 0px;" width="100%" height="100%" src="{{ section.settings.Video_link-2.sources.first.url }}" frameborder="0" allow="accelerometer; gyroscope; picture-in-picture; web-share" allowfullscreen=""></iframe>
          </div>
        {% endif %}

      </div>
    </div>
{% endif %}


{% schema %}
{
"name": "t:sections.video.name",
"tag": "section",
"class": "section",
"disabled_on": {
"groups": ["header", "footer"]
},
"settings": [
{
  "type": "text",
  "id": "title",
  "label": "Titulo de la seccion",
  "default": "#SOMOS DIABLOS <span class=\"redtxttitle\">SOMOS ROJOS</span>"
},
{
"type": "radio",
"id": "video-chanel",
"label": "Video",
"options": [
{
"value": "youtubevideo",
"label": "Youtube"
},
{
"value": "shopifyvideo",
"label": "Shopify"
},
{
  "value": "bgvideo",
  "label": "Background Video"
}
],
"default": "youtubevideo"
},
{
"type": "text",
"id": "Video_link",
"label": "Video de youtube",
"info": "Su URL debe tener la siguiente estructura: https://www.youtube.com/embed/"
},
{
"type":"video",
"id": "Video_link-2",
"label": "Video de Shopify"
},
{
  "type": "video",
  "id": "video_link-3",
  "label": "Video Background"
}
],
"presets": [
{
"name": "t:sections.video.presets.name"
}
]
}
{% endschema %}